    },
    {
      "email": "leticia.tomasini@gmail.com",
      "name": "Leticia Tomasini"
    }
  ],
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-datepicker": "^1.6.4",
    "cesium": "^1.32.1",
    "express": "^4.15.2",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "momentjs": "^2.0.0",
    "request": "^2.81.0",
    "requirejs": "^2.3.3",
    "satellite.js": "^1.3.0",
    "slick-carousel": "^1.6.0",
    "tle": "^1.2.2",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "amd-loader": "0.0.8",
    "copy-webpack-plugin": "^4.0.1",
    "cpr": "^1.1.2",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.3.0",
    "rimraf": "^2.5.2",
    "script-loader": "^0.7.0",
    "should": "^11.2.1",
    "style-loader": "^0.13.0",
    "uglify-js": "^2.8.27",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.8",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^4.1.0"
  }
